{
  "name": "@zcong/js-tracker-manager",
  "version": "0.9.0",
  "description": "my cool project",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "unpkg": "dist/index.browser.min.js",
  "cdn": "dist/index.browser.min.js",
  "jsdelivr": "dist/index.browser.min.js",
  "module": "dist/index.browser.esm.min.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production rollup -c",
    "test": "npm run lint && npm run jest",
    "test:cov": "npm run lint && npm run jest:cov",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write '**/*.{ts,json,md}'",
    "jest": "jest --forceExit",
    "jest:cov": "jest --coverage --forceExit",
    "prepublishOnly": "npm run build",
    "release": "shipjs prepare"
  },
  "types": "./dist/index.d.ts",
  "author": "zcong1993",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "esbuild": "^0.6.25",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "rollup": "^2.26.3",
    "rollup-plugin-dts": "^1.4.10",
    "rollup-plugin-esbuild": "^2.4.2",
    "shipjs": "0.20.1",
    "ts-jest": "^26.2.0",
    "typescript": "^4.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "*.{js,json,ts}": [
      "yarn format",
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && lint-staged"
    }
  },
  "dependencies": {
    "@zcong/hrtime": "^0.1.1"
  }
}
